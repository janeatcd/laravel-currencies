{
    "name": "makeabledk/laravel-currencies",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelCurrencies\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/container": "^5.4",
        "illuminate/support": "^5.4",
        "nesbot/carbon": "^1.22",
        "watson/rememberable": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "laravel/laravel": "5.5",
        "doctrine/dbal": "^2.5",
        "orchestra/testbench": "~3.5",
        "mockery/mockery": "^0.9.9"
    },
    "suggest": {
        "illuminate/database": "Required to use Eloquent Currency implementation",
        "makeabledk/laravel-production-seeding": "Setup easy seeding of your currencies across environments"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelCurrencies\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelCurrencies\\CurrenciesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "style": "./vendor/bin/php-cs-fixer fix ./src --rules=@PSR2,@Symfony,ordered_class_elements,ordered_imports --verbose"
    }
}
