{
    "name": "makeabledk/laravel-currencies",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelCurrencies\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/container": "^7.0",
        "illuminate/support": "^7.0",
        "watson/rememberable": "^3.0|^4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "laravel/laravel": "^7.0",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3"
    },
    "suggest": {
        "illuminate/database": "Required to use Eloquent Currency implementation",
        "makeabledk/laravel-production-seeding": "Setup easy seeding of your currencies across environments"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelCurrencies\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelCurrencies\\CurrenciesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
