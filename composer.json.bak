{
    "name": "phspa/laravel-currencies",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Phpsa\\LaravelCurrencies\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4.0|^8.0",
        "illuminate/container": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "laravel/laravel": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "nunomaduro/larastan": "^1.0|^2.0",
        "mockery/mockery": "^1.5",
        "calebporzio/sushi": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Phpsa\\LaravelCurrencies\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phpsa\\LaravelCurrencies\\CurrenciesServiceProvider"
            ]
        }
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit",
        "phpstan": "./vendor/bin/phpstan"
    }
